{
  "name": "consuela",
  "version": "1.0.0",
  "description": "Code analysis CLI - track exports, find unused code, analyze impact",
  "main": "dist/index.js",
  "bin": {
    "consuela": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && npm run test",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "cli",
    "code-analysis",
    "dependency-tracker",
    "dead-code",
    "unused-exports",
    "refactoring",
    "typescript"
  ],
  "author": "",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "glob": "^11.0.0",
    "inquirer": "^12.0.0",
    "ora": "^8.1.0",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.9.0",
    "@vitest/coverage-v8": "^4.0.18",
    "tsx": "^4.19.2",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
